doctype 5
html
  head
    title #{title}
    meta(name='description', content='')
    meta(name='author', content='Brandn L. Clark')
    meta(name='HandheldFriendly', content='true')
    meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
    meta(name='viewport', content='width=device-width, initial-scale=1, maximum-scale=1')


    link(rel='stylesheet', href='css/main.css')
    link(rel='stylesheet', href='//netdna.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.css')
    script(src='/socket.io/socket.io.js')
    script(src='/js/socket.js')
    script(src='/js/jquery.min.js')
    script(src='/js/jquery.easing.min.js')
    script(src='/js/jquery.scrollto.min.js')
    script(src='/js/slabtext.min.js')
    script(src='/js/jquery.nav.js')
    script(src='/js/main.js')
  body.home.color-1
    #header
      .container
        .row
          i#nav-button.icon-circle-arrow-down
          h2#logo
            a(href='/') The 
              span.highlight Mind 
              | Company
          #top-nav
            ul#fixed-nav
              if(navi)
                for navItem in navi
                  li 
                    a(href='#{navItem.location}') #{navItem.title}
  block content
